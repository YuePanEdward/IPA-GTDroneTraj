function [track_meas] = trackPrism(TPSport)
%trackPrism: use GEOCOM to track the prsim
%   Detailed explanation goes here

meas = []; 

% AUS_GetUserLockState
% Require
req = '%R1Q,18008:';
fprintf (TPSport, req);
% Reply - read response data into the sting-variable out
out = fscanf(TPSport);
% Response
res = sprintf('%R1P,0,0:RC,%s',num2str(0));
% Extract the returned values
data = regexp(out,res,'names');


% AUS_SetUserLockState
% Require
req = '%R1Q,18008:';
fprintf (TPSport, req);
% Reply - read response data into the sting-variable out
out = fscanf(TPSport);
% Response
res = sprintf('%R1P,0,0:RC,%s',);
% Extract the returned values
data = regexp(out,res,'names');

if(str2num(data.RC) == 0)   
    fprintf ('Tracking module available');
    
    % AUT_LockIn
    % Require
    req = '%R1Q,9013:';
    fprintf (TPSport, req);
    % Reply - read response data into the sting-variable out
    out = fscanf(TPSport);
    % Extract the returned values
    data = regexp(out,'%R1P,0,0:RC','names');

    if(str2num(data.RC) == 0)   
        fprintf ('Tracking module executes successful');
    else
        fprintf ('[ERROR] something wrong with the tracking');
    end
else
    fprintf ('[ERROR] Tracking module not available');
end

track_meas=meas;

end

